# Development-only defaults
# Values are intentionally safe placeholders. Override locally as needed.

TELEMETRY_ENDPOINT=http://localhost:8787/collect
TELEMETRY_ENABLED=true
TELEMETRY_FLUSH_INTERVAL_MS=2000
TELEMETRY_BATCH_SIZE=10
TELEMETRY_BUFFER_MAX_BYTES=2097152
TELEMETRY_BUFFER_MAX_FILES=3
PLACES_RATE_LIMIT_QPS=3
DATABASE_FILE_NAME=maps-list-comparator-dev.db

# Stub keys for local experiments. Replace with real secrets before shipping.
GOOGLE_PLACES_API_KEY=dev-placeholder-google
MAPTILER_API_KEY=dev-placeholder-maptiler

GOOGLE_OAUTH_CLIENT_ID=dev-placeholder-client
GOOGLE_OAUTH_CLIENT_SECRET=dev-placeholder-secret
GOOGLE_DRIVE_PICKER_PAGE_SIZE=10

# Allow dotenv loading even outside debug builds for dedicated smoke tests.
ALLOW_DOTENV=1
